// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id          Int    @id @default(autoincrement())
  title       String
  platform    String
  releaseYear Int
  slug        String
  description String
}
